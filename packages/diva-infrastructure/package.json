{
  "name": "diva-infrastructure",
  "version": "1.0.0",
  "main": "dist/main.js",
  "types": "src/main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "yarn cdktf get",
    "build": "tsc && node buildLambda.js",
    "synth": "yarn cdktf synth",
    "compile": "tsc --pretty",
    "deploy": "yarn build && cdktf deploy",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@cdktf/provider-aws": "^7.0.32",
    "cdktf": "^0.10.2",
    "constructs": "^10.0.103"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "cdktf-cli": "^0.10.2",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
